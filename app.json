{
  "expo": {
    "name": "TheSmartHelmet",
    "slug": "TheSmartHelmet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/SmartHelmet-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "splash": {
      "image": "./assets/SmartHelmet-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/smart_helmet_logo.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.bahade1233.TheSmartHelmet"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "eaa6459b-f3c8-45f4-92bf-a22a3ef16d08"
      }
    }
  }
}
